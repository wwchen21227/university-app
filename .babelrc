{
  "presets": ["@babel/preset-env", "@babel/preset-react"],
  "plugins":[
    "@babel/plugin-proposal-class-properties",
    [
      "module-resolver", {
        "alias": {
          "utils": "./src/utils",
          "constants": "./src/constants",
          "hooks": "./src/hooks",
          "testUtils": "./src/testUtils",
          "customAxios": "./src/apiUtils/customAxios"
        }
     }
    ]
  ],
  "env": {
    "test": {
      "plugins": [
        "@babel/plugin-transform-runtime"
      ]
    }
  }
}
